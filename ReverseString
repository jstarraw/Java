//this code will reverse a string 


public class MainReverse {

	
	public static void main(String [ ] args) {
		
	String s = "Hej"; 
	String rs = ""; //new reverse string 

		for(int i = s.length() - 1; i >= 0; i--) {
			rs = rs + s.charAt(i); //charAt returns the character at the specific index of a string
			
		}
		System.out.println(rs);
		
	}
}
